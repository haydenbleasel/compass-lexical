    "@lexical/markdown": "^0.6.0",
    "@lexical/react": "^0.6.0",
    "@lexical/rich-text": "^0.6.0",
    "@lexical/table": "^0.6.0",
    "@next/font": "^13.0.4",
    "@radix-ui/react-dialog": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.2",
    "@react-hookz/web": "^19.1.0",
    "@vercel/analytics": "^0.1.5",
    "@vercel/og": "^0.0.20",
    "eslint-plugin-n": "^15.5.1",
