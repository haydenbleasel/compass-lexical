{
  "name": "compass",
  "description": "A simple, ephemeral notes app.",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn:next\" \"yarn:emulators\"",
    "next": "yarn install && next dev",
    "emulators": "firebase emulators:start",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "upgrade": "npx npm-check-updates -u && yarn install"
